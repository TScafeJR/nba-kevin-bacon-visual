  "description": "This is a package for visualizing the nba players",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "audit": "npm audit --audit-level=medium --registry=https://registry.npmjs.org",
    "build-storybook": "build-storybook",
    "dev": "npm run dev:frontend",
    "dev:client": "webpack --mode development",
    "dev:frontend": "(npm run dev:client && npm run dev:webpack-server)",
    "dev:webpack-server": "webpack-dev-server --mode development",
    "lint": "eslint './*.js' './src/**/*.js' './test/**/*.js'",
    "lint:fix": "eslint --fix './*.js' './src/**/*.js' './test/**/*.js'",
    "storybook": "start-storybook -p 6006",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test mocha --recursive --timeout 10000 'test/unit/**/*.spec.js'"
  },
  "author": "Tyrone Scafe",
  "license": "ISC",
